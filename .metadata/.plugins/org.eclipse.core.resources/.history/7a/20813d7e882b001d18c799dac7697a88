package br.com.poo.sistemabancario.contas;

public class Conta {
	// Declaracao de variaveis
	public int numero;
	public String titular;
	public double saldo;
	
	//acoes da conta
	public void sacar (double valor) {
		double novoSaldo = this.saldo - valor;
		this.saldo = novoSaldo;
	}

}
//public -> publico
// protected -> protegido
// default -> padrao
// private -> privado
